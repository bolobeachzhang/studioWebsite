<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.edu.cdu.lab.model">
	<class name="Register" table="Register">
		<id name="registerId">
			<column name="registerId"></column>
		</id>
		<property name="name">
			<column name="name" length="15"  not-null="true" ></column>
		</property>
		<property name="password">
			<column name="password" length="40" not-null="true"></column>
		</property>
		<property name="sex">
			<column name="sex" length="5" not-null="false"></column>
		</property>
		<property name="idNumber">
			<column name="idNumber" length="30" not-null="false"></column>
		</property>
		<property name="birthday">
			<column name="birthday" length="20" not-null="false"></column>
		</property>
		<property name="research">
			<column name="research" length="40" not-null="false"></column>
		</property>
		<property name="phoneNumber">
			<column name="phoneNumber" length="20" not-null="false"></column>
		</property>
		<property name="workUnit">
			<column name="workUnit" length="40" not-null="false"></column>
		</property>
		<property name="email">
			<column name="email"  length="30" not-null="false"></column>
		</property>
		<property name="content">
			<column name="content" length="4000" not-null="false"></column>
		</property>
		<property name="imageUrl">
			<column name="imageUrl"  length="100" not-null="false"></column>
		</property>
	</class>
</hibernate-mapping>
