<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.edu.cdu.lab.model">
	<class name="PublicMaterial" table="publicMaterial">
		<id name="id">
			<column name="id"></column>
			<generator class="identity" />
		</id>
		<property name="name">
			<column name="name" length="50"  not-null="true" ></column>
		</property>
		<property name="size">
			<column name="size" length="10" not-null="true"></column>
		</property>
		<property name="url">
			<column name="url" length="100" not-null="true"></column>
		</property>
		<property name="type">
			<column name="type" length="10" not-null="true"></column>
		</property>
	</class>
</hibernate-mapping>
