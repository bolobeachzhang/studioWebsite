<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.edu.cdu.lab.model">
	<class name="Administrator" table="administrator">
		<id name="adminId">
			<column name="adminId" length="30"></column>
			<generator class="assigned" />
		</id>
		<property name="name">
			<column name="name" length="15" not-null="true"></column>
		</property>
		<property name="password">
			<column name="password" length="50" not-null="true"></column>
		</property>
	</class>
</hibernate-mapping>
